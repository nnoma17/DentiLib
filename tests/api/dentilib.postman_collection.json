		{
			"name": "CREATE ADMIN",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const statusCode = pm.response.code;\r",
							" \r",
							"if (statusCode !== 500) {\r",
							" \r",
							"    const reponse = pm.response.json();\r",
							"    pm.test(\"the response must be JSON\", function () {\r",
							"        pm.response.to.be.json\r",
							"    })\r",
							" \r",
							" \r",
							"    if (statusCode === 409) {\r",
							"        pm.test(\"Check if error  Message is correct\", function () {\r",
							"            pm.expect(reponse.message).to.eql(\"Email déjà utilisé.\");\r",
							"        })\r",
							"    }\r",
							"    if (statusCode === 201) {\r",
							"        pm.test(\"Check if body have properties message and user\", function () {\r",
							"            pm.expect(reponse).to.have.property(\"message\");\r",
							"            pm.expect(reponse).to.have.property(\"user\");\r",
							"        })\r",
							"    }\r",
							"    \r",
							"    if (statusCode === 400) {\r",
							"        //pm.test(\"Message d'erreur si mot de passe trop court\", function () {\r",
							"        //    if (message.includes(\"champ manquant\")) {\r",
							"        //    pm.expect(message).to.include(\"Champs manquants\");\r",
							"        //} \r",
							"        //});\r",
							"\r",
							"        if(pm.iterationData.get(\"passwordCreation\").length == 0){\r",
							"            pm.test(\"Message d'erreur si mot de passe est vide\", function () {\r",
							"                pm.expect(reponse.message).to.eql(\"Champs manquants\");\r",
							"            });\r",
							"        }else if(pm.iterationData.get(\"passwordCreation\").length < 6)\r",
							"            pm.test(\"Message d'erreur si mot de passe trop court\", function () {\r",
							"            pm.expect(reponse.message).to.eql(\"Le mot de passe doit contenir au moins 6 caractères.\");\r",
							"        });\r",
							"    }\r",
							"} else {\r",
							"    pm.test(\"respose not json\", function(){\r",
							"        pm.expect(pm.response.to.be.json).to.be.true;\r",
							"    })\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
